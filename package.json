{
  "name": "youpod",
  "version": "0.1.0",
  "description": "YouTubeチャンネルをポッドキャストに変換するツール",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "ts-node src/index.ts --max 1",
    "test:mp4": "ts-node src/index.ts --max 1 --format mp4",
    "test:yancya": "ts-node src/index.ts --max 1 --channel やんちゃクラブ",
    "test:yancya:mp4": "ts-node src/index.ts --max 1 --channel やんちゃクラブ --format mp4",
    "mp3": "ts-node src/index.ts --format mp3",
    "mp4": "ts-node src/index.ts --format mp4",
    "feed": "ts-node src/index.ts --feed-only",
    "feed:yancya": "ts-node src/index.ts --feed-only --channel やんちゃクラブ"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "fast-xml-parser": "^4.3.2",
    "fs-extra": "^11.2.0",
    "js-yaml": "^4.1.0",
    "podcast": "^2.0.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  }
}
